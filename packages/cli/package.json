{
  "name": "@logbook/cli",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EthanDM/logbook.git"
  },
  "homepage": "https://github.com/EthanDM/logbook",
  "bugs": {
    "url": "https://github.com/EthanDM/logbook/issues"
  },
  "engines": {
    "node": ">=22"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "bin": {
    "logbook": "dist/bin.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json && node -e \"require('node:fs').chmodSync('dist/bin.js', 0o755)\"",
    "dev": "tsx src/bin.ts",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "test": "node --loader tsx --test test/*.test.ts",
    "lint": "tsc -p tsconfig.json --noEmit"
  },
  "dependencies": {
    "@logbook/collector": "workspace:*",
    "@logbook/core": "workspace:*",
    "cac": "^6.7.14"
  }
}
